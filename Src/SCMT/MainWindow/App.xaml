             xmlns:local="clr-namespace:SCMTMainWindow"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Style/BaseStyle.xaml"></ResourceDictionary>
                <ResourceDictionary Source="Style/TabControlStyle.xaml"></ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
